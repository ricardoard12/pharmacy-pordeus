<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
   
   <hibernate-mapping>
   
   <class name="br.ufc.si.farmacia.model.Unidade" table="unidade">


<id name="id" column="id_unidade" >
<generator class="identity"/>
</id>

<property name="nome" column="nome_unidade"/>
<property name="nome_responsavel"  column="nome_responsavel"/>

</class>


   
   </hibernate-mapping>